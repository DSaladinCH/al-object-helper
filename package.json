{
	"name": "al-object-helper",
	"displayName": "AL Object Helper",
	"description": "VS Code Extension for the AL Language",
	"publisher": "DSaladin",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "al-object-helper.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "al-object-helper.openALFile",
				"title": "Open AL File"
			}
		],
		"keybindings": [
			{
				"command": "al-object-helper.openALFile",
				"key": "Ctrl+Alt+O"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"node-stream-zip": "^1.11.2"
	}
}
