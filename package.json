{
	"name": "al-object-helper",
	"displayName": "AL Object Helper",
	"description": "VS Code Extension for the AL Language",
	"publisher": "DSaladin",
	"version": "1.3.1",
	"icon": "Images/alLanguageHelper.png",
	"license": "MIT",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:.alpackages",
		"workspaceContains:**/*.app",
		"workspaceContains:**/app.json",
		"onLanguage:al"
	],
	"author": {
		"name": "Dominic Saladin",
		"email": "git@dsaladin.ch"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DSaladinCH/al-object-helper.git"
	},
	"bugs": {
		"url": "https://github.com/DSaladinCH/al-object-helper/issues",
		"email": "git@dsaladin.ch"
	},
	"keywords": [
		"AL",
		"AL Extensions",
		"Business Central",
		"Dynamics NAV",
		"Helper",
		"Fast Opener"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "al-object-helper.regenerate",
				"title": "AL Helper: Regenerate"
			},
			{
				"command": "al-object-helper.openALFile",
				"title": "AL Helper: Open AL File"
			},
			{
				"command": "al-object-helper.packageSearch",
				"title": "AL Helper: Package Search"
			}
		],
		"keybindings": [
			{
				"command": "al-object-helper.openALFile",
				"key": "Ctrl+Alt+O"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.40.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"adm-zip": "^0.4.16",
		"firstline": "^2.0.2",
		"jszip": "^3.5.0",
		"temp": "^0.9.1",
		"vsce": "^1.77.0"
	}
}
