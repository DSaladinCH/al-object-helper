{
	"name": "al-object-helper",
	"publisher": "DSaladin",
	"displayName": "AL Object Helper",
	"author": {
		"email": "git@dsaladin.ch",
		"name": "Dominic Saladin"
	},
	"license": "MIT",
	"description": "",
	"version": "2.1.2",
	"icon": "Images/ALObjectHelper_Small.png",
	"engines": {
		"vscode": "^1.53.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/DSaladinCH/al-object-helper.git"
	},
	"bugs": {
		"url": "https://github.com/DSaladinCH/al-object-helper/issues",
		"email": "git@dsaladin.ch"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"AL",
		"AL Extension",
		"Microsoft Business Central",
		"Quick Access",
		"Event Publishers",
		"Event Subscribers",
		"Events",
		"Definitions",
		"Hover"
	],
	"activationEvents": [
		"workspaceContains:.alpackages",
		"onLanguage:al"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "al-object-helper.openALObject",
				"title": "AL Helper: Open AL Object"
			},
			{
				"command": "al-object-helper.openALObjectOfApp",
				"title": "AL Helper: Open AL Object of App"
			},
			{
				"command": "al-object-helper.copyEvent",
				"title": "AL Helper: Copy Event"
			},
			{
				"command": "al-object-helper.jumpToEventSubscriber",
				"title": "AL Helper: Jump to local Event Subscriber"
			},
			{
				"command": "al-object-helper.openExtendedObject",
				"title": "AL Helper: Open extended Object"
			},
			{
				"command": "al-object-helper.showObjectExtension",
				"title": "AL Helper: Show Extensions"
			},
			{
				"command": "al-object-helper.reloadObjects",
				"title": "AL Helper: Reload Objects"
			},
			{
				"command": "al-object-helper.openALObjectList",
				"title": "AL Helper: Open AL Object List"
			}
		],
		"keybindings": [
			{
				"command": "al-object-helper.openALObject",
				"key": "Ctrl+Alt+O"
			},
			{
				"command": "al-object-helper.copyEvent",
				"key": "Shift+Alt+E"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "al-object-helper-objectList",
					"title": "AL Object List",
					"icon": "Images/icons/list-solid.svg"
				}
			]
		},
		"views": {
			"al-object-helper-objectList": [
				{
					"id": "alObjectHelperObjectList",
					"name": "AL Object List",
					"type": "tree"
				}
			]
		},
		"menus": {
			"commandPalette": [
				{
					"command": "al-object-helper.openExtendedObject",
					"when": "al-object-helper.isALExtension"
				},
				{
					"command": "al-object-helper.showObjectExtension",
					"when": "al-object-helper.hasALExtensions"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "AL Object Helper",
			"properties": {
				"alObjectHelper.printDebug": {
					"type": "boolean",
					"default": "false",
					"title": "Print Debug Messages",
					"description": "Print debug messages in output channel",
					"scope": "machine-overridable"
				},
				"alObjectHelper.suppressAutoReloadObjects": {
					"type": "boolean",
					"default": "true",
					"title": "Suppress auto reload Objects",
					"description": "Suppress the auto reload of objects when using a al object helper search command (Requires a fast CPU if set to false)",
					"scope": "machine-overridable"
				}
			}
		}
	},
	"moduleDirectories": [
		"rootDir"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.7",
		"@types/glob": "^7.1.1",
		"@types/line-reader": "0.0.33",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"esbuild": "^0.12.8",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"clipboardy": "^2.3.0",
		"fs-extra": "^9.1.0",
		"jszip": "^3.6.0",
		"line-reader": "^0.4.0"
	}
}
